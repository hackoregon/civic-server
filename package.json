{
  "name": "@hackoregon/civic-server",
  "version": "0.0.4",
  "description": "Server for Civic",
  "main": "index.js",
  "author": "David Daniel <davidedaniel@gmail.com> (http://davidedaniel.github.io)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hackoregon/civic-server"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bump": "bump --prompt --commit --push",
    "postbump": "editme -w",
    "build": "npm run clean && npm run build:server",
    "build:server": "babel src --out-dir lib --copy-files",
    "build:civic": "babel example --out-dir build --copy-files",
    "webpack:civic": "webpack --config ./example/civicApp/webpack.config.babel.js --colors",
    "serve:example": "node build/civicApp/index.js",
    "serve:static": "node staticServer/index.js",
    "start:ex": "run-p serve:static serve:example",
    "start:dev": "babel src -w --out-dir lib",
    "watch": "npm run start:dev",
    "clean": "rimraf ./lib ./build",
    "pre:go": "npm run clean",
    "go": "run-s build build:civic webpack:civic start:ex"
  },
  "dependencies": {
    "@hackoregon/component-library": "^0.2.0",
    "app-root-dir": "^1.0.2",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.5",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-register": "^6.23.0",
    "bunyan": "^1.8.5",
    "compression": "^1.6.2",
    "css-loader": "^0.28.0",
    "css-to-string-loader": "^0.1.2",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-json": "^2.0.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-flowtype": "2.29.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
