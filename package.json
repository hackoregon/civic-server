    "file-loader": "^0.10.1",
    "ignore-styles": "^5.0.1",
    "immutable": "^3.8.1",
    "json-immutable": "^0.2.3",
    "json-loader": "^0.5.4",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^1.3.2",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-async-component": "^0.2.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.3",
